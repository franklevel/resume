{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "/en"
    },
    {
      "source": "/:path*",
      "destination": "/en",
      "has": [
        {
          "type": "header",
          "key": "host"
        }
      ],
      "missing": [
        {
          "type": "path",
          "key": "path",
          "value": "^(en|es).*"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
